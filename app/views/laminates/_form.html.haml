= form_for(@laminate) do |f|
  - if @laminate.errors.any?
    #error_explanation
      %h2
        = pluralize(@laminate.errors.count, "error")
        prohibited this laminate from being saved:
      %ul
        - @laminate.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :lamAdhesive
    %br/
    = f.text_field :lamAdhesive
  .field
    = f.label :lamDescription
    %br/
    = f.text_area :lamDescription
  .field
    = f.label :lamLength
    %br/
    = f.number_field :lamLength
  .field
    = f.label :lamMsi
    %br/
    = f.number_field :lamMsi
  .field
    = f.label :lamType
    %br/
    = f.text_field :lamType
  .field
    = f.label :lamWidth
    %br/
    = f.number_field :lamWidth
  .actions
    = f.submit
