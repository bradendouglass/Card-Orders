= form_for(@commission) do |f|
  - if @commission.errors.any?
    #error_explanation
      %h2
        = pluralize(@commission.errors.count, "error")
        prohibited this commission from being saved:
      %ul
        - @commission.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :commrate
    %br/
    = f.number_field :commrate
  .field
    = f.label :profitrate
    %br/
    = f.number_field :profitrate
  .actions
    = f.submit
