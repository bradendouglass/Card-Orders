= form_for(@inksignature) do |f|
  - if @inksignature.errors.any?
    #error_explanation
      %h2
        = pluralize(@inksignature.errors.count, "error")
        prohibited this inksignature from being saved:
      %ul
        - @inksignature.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :vendorpartnumber
    %br/
    = f.text_field :vendorpartnumber
  .field
    = f.label :vendor
    %br/
    = f.text_field :vendor
  .field
    = f.label :magtype
    %br/
    = f.text_field :magtype
  .field
    = f.label :inktype
    %br/
    = f.number_field :inktype
  .field
    = f.label :inkmsiperpound
    %br/
    = f.number_field :inkmsiperpound
  .field
    = f.label :inkcostperpound
    %br/
    = f.number_field :inkcostperpound
  .actions
    = f.submit
