<h1>Listing contacts</h1>

<table>
  <tr>
    <th>Address1</th>
    <th>Address2</th>
    <th>City</th>
    <th>Company</th>
    <th>Email</th>
    <th>Fax</th>
    <th>Firstname</th>
    <th>Lastname</th>
    <th>Cell</th>
    <th>Mainphone</th>
    <th>State</th>
    <th>Url</th>
    <th>Zip</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @contacts.each do |contact| %>
  <tr>
    <td><%= contact.address1 %></td>
    <td><%= contact.address2 %></td>
    <td><%= contact.city %></td>
    <td><%= contact.company %></td>
    <td><%= contact.email %></td>
    <td><%= contact.fax %></td>
    <td><%= contact.firstName %></td>
    <td><%= contact.lastName %></td>
    <td><%= contact.cell %></td>
    <td><%= contact.mainPhone %></td>
    <td><%= contact.state %></td>
    <td><%= contact.url %></td>
    <td><%= contact.zip %></td>
    <td><%= link_to 'Show', contact %></td>
    <td><%= link_to 'Edit', edit_contact_path(contact) %></td>
    <td><%= link_to 'Destroy', contact, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Contact', new_contact_path %>
