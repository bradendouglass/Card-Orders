%h1 Rfid Inlay Categorys
%table{:class => "table table-striped"}
  %thead
    %tr
      %th ID
      %th
        = t("activerecord.attributes.rfid_inlay_category.inlayType", :default => t("activerecord.labels.inlayType", :default => "Inlaytype"))
      %th Created at
      %th Actions
  %tbody
    - @rfid_inlay_categories.each do |rfid_inlay_category|
      %tr
        %td= rfid_inlay_category.id
        %td= link_to rfid_inlay_category.inlayType, rfid_inlay_category_path(rfid_inlay_category)
        %td= rfid_inlay_category.created_at
        %td
          = link_to "Show", rfid_inlay_category_path(rfid_inlay_category), :class => 'btn btn-mini'
          = link_to "Edit", edit_rfid_inlay_category_path(rfid_inlay_category), :class => 'btn btn-mini'
          = link_to "Destroy", rfid_inlay_category_path(rfid_inlay_category), :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}", :class => 'btn btn-mini btn-danger' 

= link_to "New RFID Inlay Category", new_rfid_inlay_category_path, :class => 'btn btn-primary'
