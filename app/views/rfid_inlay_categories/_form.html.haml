= form_for(@rfid_inlay_category) do |f|
  - if @rfid_inlay_category.errors.any?
    #error_explanation
      %h2
        = pluralize(@rfid_inlay_category.errors.count, "error")
        prohibited this rfid_inlay_category from being saved:
      %ul
        - @rfid_inlay_category.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :inlayType
    %br/
    = f.text_field :inlayType
  .actions
    = f.submit
