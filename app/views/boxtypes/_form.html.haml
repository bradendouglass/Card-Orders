= form_for(@boxtype) do |f|
  - if @boxtype.errors.any?
    #error_explanation
      %h2
        = pluralize(@boxtype.errors.count, "error")
        prohibited this boxtype from being saved:
      %ul
        - @boxtype.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :boxid
    %br/
    = f.number_field :boxid
  .field
    = f.label :boxtype
    %br/
    = f.text_field :boxtype
  .actions
    = f.submit
