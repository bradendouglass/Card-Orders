= form_for(@dietype) do |f|
  - if @dietype.errors.any?
    #error_explanation
      %h2
        = pluralize(@dietype.errors.count, "error")
        prohibited this dietype from being saved:
      %ul
        - @dietype.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :dieDescription
    %br/
    = f.text_area :dieDescription
  .actions
    = f.submit
