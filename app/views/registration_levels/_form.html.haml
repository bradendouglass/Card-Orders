= form_for(@registration_level) do |f|
  - if @registration_level.errors.any?
    #error_explanation
      %h2
        = pluralize(@registration_level.errors.count, "error")
        prohibited this registration_level from being saved:
      %ul
        - @registration_level.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :registrationDescription
    %br/
    = f.text_area :registrationDescription
  .field
    = f.label :registrationLevelId
    %br/
    = f.number_field :registrationLevelId
  .field
    = f.label :registrationSpeed
    %br/
    = f.number_field :registrationSpeed
  .field
    = f.label :registrationWaste
    %br/
    = f.number_field :registrationWaste
  .actions
    = f.submit
