<h1>Listing registration_levels</h1>

<table>
  <tr>
    <th>Registrationdescription</th>
    <th>Registrationlevelid</th>
    <th>Registrationspeed</th>
    <th>Registrationwaste</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @registration_levels.each do |registration_level| %>
  <tr>
    <td><%= registration_level.registrationDescription %></td>
    <td><%= registration_level.registrationLevelId %></td>
    <td><%= registration_level.registrationSpeed %></td>
    <td><%= registration_level.registrationWaste %></td>
    <td><%= link_to 'Show', registration_level %></td>
    <td><%= link_to 'Edit', edit_registration_level_path(registration_level) %></td>
    <td><%= link_to 'Destroy', registration_level, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

