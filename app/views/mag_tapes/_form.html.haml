= form_for(@mag_tape) do |f|
  - if @mag_tape.errors.any?
    #error_explanation
      %h2
        = pluralize(@mag_tape.errors.count, "error")
        prohibited this mag_tape from being saved:
      %ul
        - @mag_tape.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :costPerMsi
    %br/
    = f.number_field :costPerMsi
  .field
    = f.label :costPerRoll
    %br/
    = f.number_field :costPerRoll
  .field
    = f.label :typeCate
    %br/
    = f.number_field :typeCate
  .field
    = f.label :tapeDescription
    %br/
    = f.text_field :tapeDescription
  .field
    = f.label :tapeId
    %br/
    = f.number_field :tapeId
  .field
    = f.label :tapeLength
    %br/
    = f.number_field :tapeLength
  .field
    = f.label :tapeType
    %br/
    = f.text_field :tapeType
  .field
    = f.label :tapeWidth
    %br/
    = f.number_field :tapeWidth
  .actions
    = f.submit
