= form_for(@binderyspec) do |f|
  - if @binderyspec.errors.any?
    #error_explanation
      %h2
        = pluralize(@binderyspec.errors.count, "error")
        prohibited this binderyspec from being saved:
      %ul
        - @binderyspec.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :cardsperunit
    %br/
    = f.number_field :cardsperunit
  .field
    = f.label :comment
    %br/
    = f.text_area :comment
  .field
    = f.label :numberofpersons
    %br/
    = f.number_field :numberofpersons
  .field
    = f.label :quantityperhour
    %br/
    = f.number_field :quantityperhour
  .field
    = f.label :unitmaterialcost
    %br/
    = f.number_field :unitmaterialcost
  .field
    = f.label :jobnumber
    %br/
    = f.text_field :jobnumber
  .field
    = f.label :newboxrequired
    %br/
    = f.text_field :newboxrequired
  .field
    = f.label :packageamoritizequantity
    %br/
    = f.number_field :packageamoritizequantity
  .field
    = f.label :packagecomment
    %br/
    = f.text_area :packagecomment
  .field
    = f.label :packageinnerboxid
    %br/
    = f.text_field :packageinnerboxid
  .field
    = f.label :packageouterboxid
    %br/
    = f.text_field :packageouterboxid
  .actions
    = f.submit
