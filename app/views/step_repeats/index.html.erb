    <th>Steparound</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @step_repeats.each do |step_repeat| %>
  <tr>
    <td><%= step_repeat.category %></td>
    <td><%= step_repeat.description %></td>
    <td><%= step_repeat.dimensionAcross %></td>
    <td><%= step_repeat.dimensionAround %></td>
    <td><%= step_repeat.distortionFactor %></td>
    <td><%= step_repeat.flexoDistortion %></td>
    <td><%= step_repeat.flexoLpDist %></td>
    <td><%= step_repeat.flexoMountingTapeThickness %></td>
    <td><%= step_repeat.flexoPlateBacking %></td>
    <td><%= step_repeat.flexoPlateThickness %></td>
    <td><%= step_repeat.flexoSoftThickness %></td>
    <td><%= step_repeat.gapAcross %></td>
    <td><%= step_repeat.gapAround %></td>
    <td><%= step_repeat.internalName %></td>
    <td><%= step_repeat.letterpressMountingTapeThickness %></td>
    <td><%= step_repeat.letterpressPlateBacking %></td>
    <td><%= step_repeat.letterpressPlateThickness %></td>
    <td><%= step_repeat.letterpressSoftThickness %></td>
    <td><%= step_repeat.lpDistortion %></td>
    <td><%= step_repeat.offset %></td>
    <td><%= step_repeat.offsetAmount %></td>
    <td><%= step_repeat.pressType %></td>
    <td><%= step_repeat.printDiameter %></td>
    <td><%= step_repeat.stepAcross %></td>
    <td><%= step_repeat.stepAround %></td>
    <td><%= link_to 'Show', step_repeat %></td>
    <td><%= link_to 'Edit', edit_step_repeat_path(step_repeat) %></td>
    <td><%= link_to 'Destroy', step_repeat, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

