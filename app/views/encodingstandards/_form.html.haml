= form_for(@encodingstandard) do |f|
  - if @encodingstandard.errors.any?
    #error_explanation
      %h2
        = pluralize(@encodingstandard.errors.count, "error")
        prohibited this encodingstandard from being saved:
      %ul
        - @encodingstandard.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :mchDescription
    %br/
    = f.text_area :mchDescription
  .field
    = f.label :bagging
    %br/
    = f.number_field :bagging
  .field
    = f.label :barcode
    %br/
    = f.number_field :barcode
  .field
    = f.label :hico
    %br/
    = f.number_field :hico
  .field
    = f.label :inchespersecond
    %br/
    = f.number_field :inchespersecond
  .field
    = f.label :labor
    %br/
    = f.number_field :labor
  .field
    = f.label :machineName
    %br/
    = f.text_field :machineName
  .field
    = f.label :multiplepass
    %br/
    = f.text_field :multiplepass
  .field
    = f.label :overhead
    %br/
    = f.number_field :overhead
  .field
    = f.label :runwaste
    %br/
    = f.number_field :runwaste
  .field
    = f.label :variabledata
    %br/
    = f.number_field :variabledata
  .field
    = f.label :secondsperhour
    %br/
    = f.number_field :secondsperhour
  .actions
    = f.submit
