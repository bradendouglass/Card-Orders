= form_for(@ink_varnish) do |f|
  - if @ink_varnish.errors.any?
    #error_explanation
      %h2
        = pluralize(@ink_varnish.errors.count, "error")
        prohibited this ink_varnish from being saved:
      %ul
        - @ink_varnish.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :inkCostPerPound
    %br/
    = f.number_field :inkCostPerPound
  .field
    = f.label :inkMsiPerPound
    %br/
    = f.number_field :inkMsiPerPound
  .field
    = f.label :vendor
    %br/
    = f.text_field :vendor
  .field
    = f.label :vendorPartNumber
    %br/
    = f.text_field :vendorPartNumber
  .actions
    = f.submit
